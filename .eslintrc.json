{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "google",
        "eslint-config-prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "eslint-plugin-prettier",
        "@typescript-eslint"
    ],
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine":"auto"
        }]
    }
}
